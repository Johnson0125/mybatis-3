<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
        PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
    <properties>
        <property name="username" value="root"/>
        <property name="password" value="123456"/>
        <property name="test" value=""></property>
        <property name="" value=""></property>
    </properties>
    <settings>
        <setting name="mapUnderscoreToCamelCase" value="true"/>
    </settings>
    <typeAliases>
        <typeAlias alias = "Student" type = "shfq.Student"/>
        <typeAlias alias = "" type = "shfq.Cat"/>
        <typeAlias type = "shfq.Dog"/>
        <package name="shfq.vo"/>
    </typeAliases>
    <environments default="development">
        <environment id="development">
            <transactionManager type="JDBC"/>
            <dataSource type="POOLED">
                <property name="driver" value="com.mysql.jdbc.Driver"/>
                <property name="url" value="jdbc:mysql://localhost:3306/mybatis-demo"/>
                <property name="username" value="root"/>
                <property name="password" value="123456"/>
            </dataSource>
        </environment>
    </environments>
    <mappers>
        <!--<mapper resource="shfq/mappers/StudentUpdateMapper.xml"/>-->
        <mapper class="shfq.StudentMapperTest"></mapper>
    </mappers>
</configuration>